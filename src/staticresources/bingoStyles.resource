.bingoitem {
    float:left;
    width:120px;
    border:1px solid #000000;
    height: 120px;
    background-color:#eeeeee;
    text-align: center;
    vertical-align: middle;
    word-wrap: break-word;
}

.clicked {
    font-weight: bold;
    background-color: #0070d2;
    color:white;
    text-transform: uppercase;
}

.cardname {
    vertical-align: middle;
}